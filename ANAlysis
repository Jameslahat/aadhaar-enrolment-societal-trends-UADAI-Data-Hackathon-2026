   "source": [
    "# Converting date column to datetime\n",
    "df['date'] = pd.to_datetime(df['date'], format=\"%d-%m-%Y\")\n",
    "\n",
    "# Created total enrolments column\n",
    "df['total_enrolments'] = (\n",
    "    df['age_0_5'] +\n",
    "    df['age_5_17'] +\n",
    "    df['age_18_greater']\n",
    ")\n",
    "\n",
    "# Extracting year and month\n",
    "df['year'] = df['date'].dt.year\n",
    "df['month'] = df['date'].dt.month\n",
    "\n",
    "df.head()\n"
   ]
